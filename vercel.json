{
  "version": 2,
  "builds": [
    {
      "src": "main.py",  // Thay nếu file tên server.py hoặc api/main.py
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }  // Hoặc python3.11 nếu bạn dùng cũ hơn
    }
  ],
  "routes": [
    { "src": "/collect", "dest": "main.py" },
    { "src": "/test-log", "dest": "main.py" },
    { "src": "/(.*)", "dest": "main.py" }
  ]
}
